# Healthcare CDSS - Environment Configuration
# Copy this file to .env and fill in your API keys

# Anthropic API Key (required for Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LangSmith API Key (required for tracing and evaluation)
LANGSMITH_API_KEY=your_langsmith_api_key_here

# LangSmith Project Name (optional, defaults to "healthcare-cdss")
LANGCHAIN_PROJECT=healthcare-cdss

# LangSmith Tracing (set to "true" to enable)
LANGCHAIN_TRACING_V2=true

# LangSmith Endpoint (usually don't need to change)
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
